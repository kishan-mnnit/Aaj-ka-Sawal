bool isSubsequence(string s, string t) {
        int ptr1=0;
        int ptr2=0 ;
        for(int i=0;i<t.size();i++){
            if(s[ptr1]==t[ptr2]){
                ptr1+=1;
                ptr2+=1;
            }
            else
                ptr2+=1;
                
        }
        if(ptr1==s.size())
            return true;
        else 
            return false;
    }
